{
  "name": "ctsj-vue",
  "version": "1.0.0",
  "description": "一个对vue进行的模拟",
  "main": "lib/vue.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "startapp": "ctbuild startapp",
    "buildapp": "ctbuild buildapp",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "link": "npm link @ctsj/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playerljc/CTSJ-VUE.git"
  },
  "author": "playerljc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/playerljc/CTSJ-VUE/issues"
  },
  "homepage": "https://github.com/playerljc/CTSJ-VUE#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/runtime": "^7.7.4",
    "@umijs/fabric": "^2.0.8",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.0",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "snabbdom": "^2.1.0",
    "lodash": "^4.17.20",
    "typescript": "^3.8.2"
  }
}
